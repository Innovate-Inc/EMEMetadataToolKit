<UserControl x:Class="CustomMetadataEditor.Pages.Extent"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:editor="clr-namespace:ESRI.ArcGIS.Metadata.Editor;assembly=ESRI.ArcGIS.MetadataEditor.Controls"
             xmlns:pages="clr-namespace:CustomMetadataEditor.Pages">
  
    <Grid>
      <editor:HLabel Content="Geographic Description"/>
      <editor:TextArea Text="{Binding XPath=temp_path, Mode=TwoWay}"></editor:TextArea>

      <editor:H2>Bounding Coordinates</editor:H2>

      <editor:HLabel Content="West"/>
      <editor:TextBox Text="{Binding XPath=temp_path}" ></editor:TextBox>



      <editor:HLabel Content="East"/>
      <editor:TextBox Text="{Binding XPath=temp_path}" ></editor:TextBox>



      <editor:HLabel Content="North"/>
      <editor:TextBox Text="{Binding XPath=temp_path}" ></editor:TextBox>

      <editor:HLabel Content="South"/>
      <editor:TextBox Text="{Binding XPath=temp_path}" ></editor:TextBox>      
    </Grid>
</UserControl>
